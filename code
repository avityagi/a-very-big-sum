#include <iostream>
using namespace std;
int main()
{
    int n;
    long long int a[n];
    cin>>n;
    for (int i=0; i<n ; i++) 
    {
    cin>>a[i];
    }
    for (int i=1; i<n ; i++) 
    {
    a[i]=a[i-1]+a[i];
    }
    cout<<a[n-1];
    return 0;
}
//inputs
//5
//1000000001 1000000002 1000000003 1000000004 1000000005
//but output is
//4198628
